
[core]
	editor = jed

[user]
	name = Peter Malmberg
	email = peter.malmberg@gmail.com

#[diff]
#  external = ~/pmg/bin/git_diff hook_default

[push]
  default = simple

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short 
  type = cat-file -t 
  dump = cat-file -p
  deleted = log --diff-filter=D --summary

[init]
	defaultBranch = master

[pull]
  rebase = false
[diff]
	external = /home/main/bin/git_diff_meld.py
