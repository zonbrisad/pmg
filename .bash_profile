CHARSET=latin1
export CHARSET


# Get the aliases and functions
if [ -f ~/.bashrc ]; then
  . ~/.bashrc
fi
				
export BASH_ENV=$HOME/.bashrc
				
				
# Cygwin settings
export LANG=sv
				


source ~/.xsh

