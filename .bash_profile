


# language settings for different software

LC_ALL=C
export LC_ALL

LANGUAGE=sv_SE
export LANGUAGE

LANG=sv_SE
export LANG

LESSCHARSET=latin1
export LESSCHARSET

MM_CHARSET=iso-8859-1
